        #   ...
        assert.deepEq({
          localDefs: [],
          funcs: [],
          stmts: [{
            tryBlock: {
              localDefs: [],
              funcs: [],
              stmts: [{
                expr: {
                  callee: {
                    refId: npcbUid,
                  },
                  args: [],
                },
              }],
            },
            catchBlock: {
              expr: {
                callee: {
                  refId: catchUid,
                },
                args: [{}],
              },
            },
            excRef: {},
          }],
        }, arg.asyncFlow)

        assert.deepEq({
          throwing: false,
          catchFunc: r.funcs[0],
        }, arg.throwMethod)
        done()

