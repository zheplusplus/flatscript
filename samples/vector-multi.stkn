func vector(x, y, z)
    func multi(x0, y0, z0)
        return x * x0 + y * y0 + z * z0
    return multi

func calc(a, b, vec, c)
    return vec(a, b, c)

func calc2(a, b, vec, c)
    return calc(a, b, vec, c)

